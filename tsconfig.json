{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "lib": [
      "es6"
    ],
    "rootDir": ".",
    "esModuleInterop": true,
    "outDir": "build",
    "allowSyntheticDefaultImports": true,
    "declaration": true
  },
  "include": [
    "index.ts",
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "tests",
    "example"
  ]
}
